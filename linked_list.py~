
# linked list
# in Pythonic way


#!/bin/env/python

class Node:

    def __init__(self, cargo=None, next=None):

        self.cargo = cargo
        self.next = next



    def __str__(self):
        return str(self.cargo)


    def printCurrent(self):
        return self.cargo




class LinkedList:

    def __init__(self, current=None):
        sef.current = current
        
